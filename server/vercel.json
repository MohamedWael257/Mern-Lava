{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "server"
        },
        {
            "src": "/api/(.*)",
            "dest": "api/$1"
        }
    ]
}